<h1>
  Product List
</h1>

<table>
  <thead>
    <tr>
      <th scope='col'>商品名稱</th>
      <th scope='col'>商品價錢</th>
      <th scope='col'>商品數量</th>
      <th scope='col'></th>
    </tr>
  </thead>
  <% @products.each do |product| %>
    <tbody>
      <td><%= product.title %></td>
      <td><%= product.list_price %></td>
      <td><%= product.quantity %></td>
      <td><%= link_to '查看商品', store_product_path(product.store_id, product) %></td>
    </tbody>
  <% end %>
</table>
